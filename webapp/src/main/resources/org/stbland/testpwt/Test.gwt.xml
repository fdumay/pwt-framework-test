<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN" "https://raw.githubusercontent.com/gwtproject/gwt/master/distro-source/core/src/gwt-module.dtd">
<module rename-to="test">

    <inherits name="fr.putnami.pwt.core.widget.Widget" />
    <inherits name="fr.putnami.pwt.core.mvp.Mvp"/>
    <inherits name="fr.putnami.pwt.core.model.Model"/>
    <inherits name="fr.putnami.pwt.core.service.Service"/>    
    <inherits name="fr.putnami.pwt.core.inject.Inject"/>
    <!--
    <inherits name="fr.putnami.pwt.core.security.Security"/>    
    -->

    <source path="client" />
    <source path="shared" />
     
    <entry-point class="org.stbland.testpwt.client.Main" />
        
    <extend-property name="locale" values="fr"/>
    
    <set-property name="user.agent" value="safari,gecko1_8" />    
        
    <!-- This linker will be the default one in later version of GWT -->
    <add-linker name="xsiframe" />        
</module>